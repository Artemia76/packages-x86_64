run=()



name=nnn
version=1.5
release=1

source=(https://github.com/jarun/$name/archive/v$version.tar.gz
)

prepare() {
echo "Fonction vide, normal..."
}

build() {
cd $name-$version
make PREFIX=/usr
make PREFIX=/usr DESTDIR=$PKG install
}

package() {
echo "Fonction  vide, normal..." 
}

uptodate() {
echo "Fonction  vide, normal..." 
}

