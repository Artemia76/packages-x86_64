description="Python 2 compatibility utilities."
url="http://pypi.python.org/pypi/six/"
contributors="tnut"
packager="greg<greg@nutyx.org>"

# Depends on: python2

name=python2-six
version=1.11.0
release=1

source=(https://pypi.python.org/packages/16/d8/bc6316cf98419719bd59c91742194c111b6f2e85abac88e496adefaf7afe/six-1.11.0.tar.gz)

build() {

    cd six-$version
    python2 setup.py check
    python2 setup.py install --prefix=/usr --root=$PKG
    
}
