description="Implementation of lex and yacc parsing tools"
url="http://www.dabeaz.com/ply/"

packager="tnut <tnut@nutyx.org>"
contributors="Remy"

run=(python2)

name=python2-ply
version=3.11

source=(${url}ply-$version.tar.gz)

build() {

cd ply-$version
python2 setup.py install --prefix=/usr --root=$PKG
}
