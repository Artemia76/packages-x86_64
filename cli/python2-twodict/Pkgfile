# Depends on: python2

description="Simple two way ordered dictionary for Python"
url="https://github.com/MrS0m30n3/twodict"
packager="rems@nutyx.org"

name=python2-twodict
version=1.2

PKGMK_KEEP_SOURCES="no"
source=(https://github.com/MrS0m30n3/twodict/archive/$version.tar.gz)


build() {
cd twodict-$version
python2 setup.py install --root=$PKG/ --optimize=1
}

