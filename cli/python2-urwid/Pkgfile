description="Urwid is a curses-based user interface library for Python."
url="http://excess.org/urwid/"
contributors="tnut"
packager="greg<greg@nutyx.org>"

# Depends on: python2-setuptools

name=python2-urwid
version=2.0.1
release=1

source=(https://files.pythonhosted.org/packages/c7/90/415728875c230fafd13d118512bde3184d810d7bf798a631abc05fac09d0/urwid-2.0.1.tar.gz)

build() {
    cd urwid-$version
    python2 setup.py install --prefix=/usr --root=$PKG
}


