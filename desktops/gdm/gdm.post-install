. lib/lsb/init-functions

if [ ! -L etc/systemd/system/display-manager.service ];then
   mkdir -p etc/systemd/system
   ln -sf /lib/systemd/system/gdm.service etc/systemd/system/display-manager.service && \
   log_success_msg2  "${INFO} Created symlink /etc/systemd/system/display-manager.service -> /lib/systemd/system/gdm.service${NORMAL}"
else
   log_warning_msg "${WARNING} symlink /etc/systemd/system/display-manager.service already exist${NORMAL}"
fi
