source=(https://az764295.vo.msecnd.net/stable/b06ae3b2d2dbfe28bca3134cc6be65935cdfea6a/code_1.69.1-1657615746_amd64.deb)

build() {
   tar -xf data.tar.xz -C $PKG/

   mkdir -p $PKG/usr/bin
   ln -sv /usr/share/code/bin/code $PKG/usr/bin/code
}