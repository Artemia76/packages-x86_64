description="A library for online banking and financial applications"
url="https://www.aquamaniac.de/rdm/projects/aqbanking"

packager="tnut <tnut@nutyx.org>"
contributors=""

makedepends=(gwenhywfar xmlsec)
run=()
set=()

name=aqbanking
version=6.0.2

source=(https://www.aquamaniac.de/rdm/attachments/download/243/$name-$version.tar.gz)

build() {
cd $name-$version
export PKG_CONFIG=/usr/bin/pkg-config
./configure --prefix=/usr --enable-gwenhywfar
make
make DESTDIR=$PKG install
}
