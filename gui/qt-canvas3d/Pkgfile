# Depends on: qt-base qt-declarative

description="A JavaScript 3D rendering API for Qt Quick"
url="http://qt-project.org/"
packager="pierre at nutyx dot org"

name=qt-canvas3d 
release=2
alias=(qtcanvas3d)
_name="${name/-/}-opensource-src"
build() {
cd ${_name}-${version}
qmake
make
make INSTALL_ROOT=$PKG install
}
