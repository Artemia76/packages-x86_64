# Depends on: qt-base qt-declarative qt-tools libtiff libmng 

description="A cross-platform application and UI framework, QtTranslations files"
url="http://qt-project.org/"
packager="pierre at nutyx dot org"

name=qt-translations 
version=5.8.0-rc
release=1

_name="${name/-/}-opensource-src-${version}"

source=(https://download.qt.io/development_releases/qt/5.8/$version/submodules/${_name}.tar.xz)

build() {
cd ${_name}
qmake-qt
make
make INSTALL_ROOT=$PKG install
}
