# Depends on: qt-base

description="A cross-platform application and UI framework, QtWinextras files"
url="http://qt-project.org/"
packager="pierre at nutyx dot org"

name=qt-x11extras
release=2
alias=(qtx11extras)
_name="${name/-/}-opensource-src"
build() {
cd ${_name}-${version}
qmake
make
make INSTALL_ROOT=$PKG install
}
