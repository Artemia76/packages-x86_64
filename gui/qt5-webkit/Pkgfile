# Depends on: qt5-base qt5-location qt5-sensors qt5-webchannel qt5-doc qt5-tools ruby icu libjpeg-turbo libpng libwebp libxslt 

description="Classes for a WebKit2 based implementation and a new QML API"
url="http://qt-project.org/"
packager="pierre at nutyx dot org"

name=qt5-webkit 
version=5.8.0-rc
name=qt5-webkit
version=5.8.0-beta
release=1

_name=qtwebkit-opensource-src-$version

source=(http://download.qt.io/community_releases/5.8/$version/${_name}.tar.xz
        qt5-webkit-default_prf.patch)

build() {
  cd ${_name}
  patch -p1 -i ../qt5-webkit-default_prf.patch
  qmake-qt5
  make
  make INSTALL_ROOT="$PKG" install
}
