# Description : Parts of framework 5
# URL: https://www.kde.org/
# Packager: pierre at nutyx dot org
# Depends on: cmake doxygen python-jinja2 python-pyyaml
name=kapidox
release=1
version=5.12.0
_version=5.12
source=(http://download.kde.org/stable/frameworks/${_version}/$name-$version.tar.xz)

build() {
source /usr/bin/setqt5 &&
cd $name-$version
mkdir build
cd    build
cmake -DCMAKE_INSTALL_PREFIX=/usr ..
make
make DESTDIR=$PKG install
}

