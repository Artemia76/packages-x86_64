_name=plasma-integration
release=2

build() {
cd ${_name}-$version
mkdir build
cd    build
cmake -DCMAKE_INSTALL_PREFIX=/usr \
      -DCMAKE_BUILD_TYPE=Release  \
      -DLIB_INSTALL_DIR=lib       \
      -DBUILD_TESTING=OFF         \
      -Wno-dev ..
make
make DESTDIR=$PKG install
}
