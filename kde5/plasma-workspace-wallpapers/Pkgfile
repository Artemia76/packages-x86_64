# Description : Parts of framework 5
# URL: https://www.kde.org/
# Packager: pierre at nutyx dot org
# Depends on: kf5-extra-cmake-modules

name=plasma-workspace-wallpapers
release=1
version=5.3.2
_version=5.3.2
source=(http://download.kde.org/stable/plasma/${_version}/$name-$version.tar.xz)

build() {
cd $name-$version
mkdir build
cd    build
cmake -DCMAKE_INSTALL_PREFIX=/usr \
       -Wno-dev ..
make
make DESTDIR=$PKG install
}

